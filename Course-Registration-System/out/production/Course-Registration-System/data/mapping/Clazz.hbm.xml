<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="data.model.Clazz" table="class" schema="public" catalog="course-registration-db">
        <id name="classId">
            <column name="class_id" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="maleStudents">
            <column name="male_students" sql-type="smallint"/>
        </property>
        <property name="femaleStudents">
            <column name="female_students" sql-type="smallint"/>
        </property>
    </class>
</hibernate-mapping>