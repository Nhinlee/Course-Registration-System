<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="data.model.CourseRegistrationSession" table="course_registration_session" schema="public"
           catalog="course-registration-db">
        <id name="sessionId">
            <column name="session_id" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="startDate">
            <column name="start_date" sql-type="date"/>
        </property>
        <property name="endDate">
            <column name="end_date" sql-type="date"/>
        </property>
        <property name="semesterId">
            <column name="semester_id" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>